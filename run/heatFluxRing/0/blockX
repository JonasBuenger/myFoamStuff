FoamFile
{
    version     2.0;
    format      ascii;
    class       volVector4Field;
    location    "0";
    object      blockX;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0 0);

boundaryField
{
    up
    {
        type            calculated;
        value           uniform (0 0 0 0);
    }
    hole
    {
        type            calculated;
        value           uniform (0 0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "2";
    object      vc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<vector> 3((1 10 100) (2 20 200) (3 30 300));

boundaryField
{
    boundary
    {
        type            zeroGradient;
    }
}



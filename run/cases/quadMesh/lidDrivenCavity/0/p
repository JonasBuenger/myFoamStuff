FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    lid
    {
        type            implicitExtrapolation;
        value           uniform 1;
    }
    cavity
    {
        type            implicitExtrapolation;
        value           uniform 1;
    }
    frontAndBack
    {
       type            empty;
    }

}


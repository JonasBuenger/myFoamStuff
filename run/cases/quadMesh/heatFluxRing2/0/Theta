FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    up
    {
        	//type  fixedValue; 
			type  myTemperature2;
			value uniform 1;
    }
    hole
    {
        	//type  fixedValue; 
			type  myTemperature2;
			value uniform 4;
    }
    frontAndBack
    {
        	type empty;
    }
}


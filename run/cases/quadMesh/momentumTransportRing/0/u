FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      u;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    up 
    {
                //type    myVelocity;
                //type    myImplicitVelocity;
					 type		fixedValue;
                value   uniform (1 0 0);
    }
    hole
    {
                //type zeroGradient;
                type		myImplicitVelocity;
                value	uniform (1 0 0);
    }
    frontAndBack
    {
       type            empty;
    }
}


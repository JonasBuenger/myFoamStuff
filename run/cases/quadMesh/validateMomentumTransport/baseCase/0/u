FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      u;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    out
    {
                type		fixedValue;
                value           uniform (1 0 0);
    }
    in
    {
                type            fixedValue;
                //type            myVelocity;
                value           uniform (0 0 0);
    }
    frontAndBack
    {
       type            empty;
    }
}


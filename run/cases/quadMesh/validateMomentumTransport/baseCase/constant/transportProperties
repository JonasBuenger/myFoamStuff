Kn              Kn      [ 0 0 0 0 0 0 0 ] 0.1; // gleich 1 gesetzt
epsilonL        epsilonL [ 0 0 0 0 0 0 0 ] 0.1; // Faktor Diffusion-Stabilisierung U
epsilonLR       epsilonLR [ 0 0 0 0 0 0 0 ] 1; // Faktor Diffusion-Stabilisierung U
epsilonSigma    epsilonSigma [ 0 0 0 0 0 0 0 ] 0; // Faktor Diffusion-Stabilisierung U
divSigma    divSigma [ 0 0 0 0 0 0 0 ] -1; // Faktor Diffusion-Stabilisierung U


FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "80";
    object      Theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    up
    {
        type            myTemperature;
        value           uniform 1;
    }
    hole
    {
        type            myTemperature;
        value           uniform 4;
    }
    frontAndBack
    {
        type            empty;
    }
}



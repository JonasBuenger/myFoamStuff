FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outsideCircle
    {
        type            myHeatFlux;
        alpha	        1;
	Theta_wall	1;
	gamma		1;
	g0		(0 0 0);
	value		uniform (0 0 0);
    }
    insideCircle 
    {
        type            myHeatFlux;
        alpha	        1;
  	Theta_wall	4;
	gamma		1;
	g0		(30 0 0);
	value		uniform (0 0 0);
    }
    topAndBottom 
    {
       type            empty;
    }
}


FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(Theta)         Gauss linear;
}

divSchemes
{
    default     Gauss linear;
    div(s)      Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
    laplacian(nu,s) Gauss linear corrected;//orthogonal;
}

interpolationSchemes
{
    default         linear;
//    interpolate(HbyA) linear;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    Theta           ;
}


